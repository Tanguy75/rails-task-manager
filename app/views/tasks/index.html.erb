<h1> My tasks </h1>
<ul>
  <% @tasks.each do |task| %>
 <li> <input type="checkbox" id="laundry" name="laundry" checked>
  <%= link_to task.title, task_path(task) %>
  <%= link_to "Delete", task_path(task),
      method: :delete,
      data: { confirm: "Are you sure?" } %>
<% end %> </li>
</ul>

<%=link_to "Add a new task", tasks_new_path%>


